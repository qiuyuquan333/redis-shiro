<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qyq.springbootshiro.mapper.AdminMapper">

    <select id="findAdminByName" parameterType="String" resultType="com.qyq.springbootshiro.pojo.Admin">
        SELECT * FROM admin WHERE aname = #{name};
    </select>
</mapper>